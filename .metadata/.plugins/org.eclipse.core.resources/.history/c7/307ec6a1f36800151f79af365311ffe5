import java.util.concurrent.ThreadLocalRandom;

public class Process {
	private int privKey;
	private int pubKey;
	private int prodKey;
	
	
	private static int genprivateNum(){
		int min = 10;
		int max = 100;
		int ran;
		ran = ThreadLocalRandom.current().nextInt(min, max + 1);
		return ran;
	}
	
	private static int genpublicNum(){
		int min = 1000;
		int max = 10000;
		int ran;
		ran = ThreadLocalRandom.current().nextInt(min, max + 1);
		return ran;
	}
}
